{
    "compilerOptions": {
        "module": "esnext",
        "target": "es2015",
        "lib": ["es2020", "dom"],
        "sourceMap": true,
        "jsx": "react",
        "moduleResolution": "node",
        "strict": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "resolveJsonModule": true
    },
    "exclude": ["node_modules"]
}

{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",

    "rushVersion": "5.42.4",

    "pnpmVersion": "5.18.1",
    "pnpmOptions": {
        "pnpmStore": "local",
        "strictPeerDependencies": true,
        "resolutionStrategy": "fewer-dependencies",
        "preventManualShrinkwrapChanges": true,
        "useWorkspaces": true
    },

    "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0",
    "suppressNodeLtsWarning": false,

    "ensureConsistentVersions": true,

    "projectFolderMinDepth": 2,
    "projectFolderMaxDepth": 2,

    "allowMostlyStandardPackageNames": false,

    "gitPolicy": {
        "versionBumpCommitMessage": "Applying package updates. [skip-ci]",
        "changeLogUpdateCommitMessage": "Deleting change files and updating change logs for package updates. [skip-ci]"
    },

    "repository": {
        "url": "https://gitlab.com/Alduino/music-app",
        "defaultBranch": "master",
        "defaultRemote": "origin"
    },

    "eventHooks": {
        "preRushInstall": [],
        "postRushInstall": [],
        "preRushBuild": [],
        "postRushBuild": []
    },

    "variants": [],

    "telemetryEnabled": true,

    "hotfixChangeEnabled": false,

    "projects": [
        {
            "packageName": "@muzik/app-desktop",
            "projectFolder": "apps/desktop"
        }
    ]
}

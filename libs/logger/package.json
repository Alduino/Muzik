{
    "name": "@muzik/logger",
    "version": "1.0.0",
    "description": "Automatic log child creation",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "muzik-bundle -f cjs,esm -t types -e src/index.ts -o dist/index",
        "build:watch": "muzik-bundle --watch -f cjs,esm -t types -e src/index.ts -o dist/index"
    },
    "dependencies": {
        "@roarr/middleware-serialize-error": "^1.0.0",
        "roarr": "^4.2.5",
        "tslib": "^2.1.0"
    },
    "devDependencies": {
        "@muzik/config-bundler": "workspace:^1.0.0"
    }
}

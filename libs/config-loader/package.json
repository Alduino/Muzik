{
    "name": "@muzik/config-loader",
    "version": "1.0.0",
    "description": "Reads and writes YAML config files from correct directories",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "muzik-bundle -f cjs,esm -t types -e src/index.ts -o dist/index",
        "build:watch": "muzik-bundle --watch -f cjs,esm -t types -e src/index.ts -o dist/index"
    },
    "dependencies": {
        "tslib": "^2.1.0",
        "yaml": "^1.10.2"
    },
    "devDependencies": {
        "@babel/core": "^7.13.14",
        "@types/node": "^14.14.37",
        "@muzik/config-bundler": "workspace:^1.0.0"
    }
}
